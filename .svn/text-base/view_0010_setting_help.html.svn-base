				</li>
			</ul>
		</div>
	</body>

</html>
<script>
	function gotoPage(url) {
		mui.openWindow({
			url: url,
			id: url
		});
	}

	function preloadPage() {
		mui.preload({
			url: 'view_0010_setting_help.html',
			id: 'view_0010_setting_help.html'
		});
		mui.preload({
			url: 'view_0010_setting_help_shopprocess.html',
			id: 'view_0010_setting_help_shopprocess.html'
		});
		mui.preload({
			url: 'view_0010_setting_help_useragreement.html',
			id: 'view_0010_setting_help_useragreement.html'
		});
		mui.preload({
			url: 'view_0010_setting_help_privacystatement.html',
			id: 'view_0010_setting_help_privacystatement.html'
		});
		mui.preload({
			url: 'view_0010_setting_help_about.html',
			id: 'view_0010_setting_help_about.html'
		});
	}

	mui.plusReady(function() {
		var view = plus.webview.currentWebview();
		view.addEventListener("show", function(e) {
			console.log("Webview Showed：" + view.id);
			preloadPage();
		}, false);
		mui("body").on('tap', '.goto-page', function() {
			var href = this.getAttribute('data-href');
			gotoPage(href);
		});
	});
</script>
<script src="js/immersed.js"></script>